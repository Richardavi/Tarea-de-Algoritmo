//12. Escribir "números 1 al 12 Con lazo mientras"
Algoritmo Lazos
	Escribir "Numeros 1 al 12 con lazo Mientras"
	Mientras i<=11 Hacer
		i=i+1
		Escribir i
	Fin Mientras
	
	Escribir "Numeros 1 al 12 con Lazo Repetir"
	Repetir
		a=a+1
		Escribir a
	Hasta Que a>=12
	
	Escribir "Numeros 1 al 12 con Lazo Para"
	Para q<-1 Hasta 12 Hacer
		Escribir q
	Fin Para
	
FinAlgoritmo
